<% include _layout/header %>

<div class="row products" style="margin-top: 20px; margin-bottom: 20px;">
    
       
        <% products.forEach(function(p){ %>
                <div class="col-xs-12 col-md-4 p" style="margin-top: 20px; margin-bottom: 20px;">
                    <a style="display: inline-block;" class="pa" href="/products/<%= p.category %>/<%= p.slug %>">
                        <img style="width: 150px;;" class="pimage" src="/product_images/<%= p.id %>/<%= p.image %>" alt="">
                    </a>
                    <h3 style="font-family: 'Ubuntu', sans-serif; font-size: 20px"><%= p.title %></h3>
                    <h4 style="font-family: 'Ubuntu', sans-serif; font-size: 20px">$<%= parseFloat(p.price).toFixed(2) %></h4>
                    <a class="vd" href="/products/<%= p.category %>/<%= p.slug %>" style="font-family: 'Ubuntu', sans-serif;font-family: 'Ubuntu', sans-serif;">View Details</a>
                </div>
            <% }); %>
        
    </div>

<% include _layout/footer %>